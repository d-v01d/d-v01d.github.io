{
  "version": 2,
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "index.html", "status": 404 }
  ],
  "builds": [{ "src": "package.json", "use": "@now/static-build" }],
  "build": {
    "env": {
      "NEXT_PUBLIC_SENTRY_DSN": "@NEXT_PUBLIC_SENTRY_DSN",
      "GA_CODE": "@ga_code"
    }
  }
}
