{
  "version": 2,
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "index.html", "status": 404 }
  ],
  "builds": [
    { "src": "package.json", "use": "@now/static-build" }
  ],
  "build": {
    "env": {
      "ROLLBAR_TOKEN": "@rollbar_token",
      "GA_CODE": "@ga_code"
    }
  }
}
